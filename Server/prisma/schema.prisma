generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        Int      @id @default(autoincrement())
  name      String?
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}

model Turf {
  id        String   @id @default(uuid())
  name      String
  city      String
  address   String
  ownerId   String
  ownerName String
  rent      Float
  image     String
  slots     Json     @default("{}")
  createdAt DateTime @default(now())
}
